@model string[,]
@{
    ViewBag.Title = "Consulta";
}

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Cantidad</th>
            </tr>
        </thead>
        <tbody>
            @{ int cant = Model.GetUpperBound(0); }
            @for (int i = 0; i <= cant; i++)
            {
                <tr>
                    <td>@(Model[i, 0])</td>
                    <td>@(Model[i, 1])</td>
                    <td>@(Model[i, 2])</td>
                </tr>
            }
        </tbody>
    </table>
</div>